{
  "name": "SMART Staff Produtivo",
  "description": "Build an Extension!",
  "version": "1.0",
  "manifest_version": 3,
  "options_page": "options.html",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": ["storage", "activeTab", "scripting", "clipboardRead", "clipboardWrite", "webRequest"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/get_started16.png",
      "32": "assets/get_started32.png",
      "48": "assets/get_started48.png",
      "128": "assets/get_started128.png"
    }
  },
  "icons": {
    "16": "assets/get_started16.png",
    "32": "assets/get_started32.png",
    "48": "assets/get_started48.png",
    "128": "assets/get_started128.png"
  }
}